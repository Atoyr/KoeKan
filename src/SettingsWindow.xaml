<Window x:Class="Medoz.KoeKan.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Settings" Height="200" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock Text="Settings" FontSize="16" FontWeight="Bold" Margin="10"/>
            <TextBlock Text="Setting 1:" Margin="10,10,10,0"/>
            <TextBox x:Name="Setting1TextBox" Margin="10"/>
            <Button Content="OK" Width="100" Margin="10" Click="OkButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
